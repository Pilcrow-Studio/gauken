{
  "format": "page",
  "id": "front_page",
  "label": "Front Page",
  "repeatable": false,
  "status": true,
  "json": {
    "Main": {
      "uid": {
        "type": "UID",
        "config": {
          "label": "UID",
          "placeholder": ""
        }
      },
      "title": {
        "type": "Text",
        "config": {
          "label": "Title",
          "placeholder": ""
        }
      },
      "introductory_text": {
        "type": "StructuredText",
        "config": {
          "label": "Introductory Text",
          "placeholder": "",
          "allowTargetBlank": true,
          "multi": "paragraph,strong,em,image,list-item,o-list-item,hyperlink"
        }
      },
      "slices": {
        "type": "Slices",
        "fieldset": "Slice Zone",
        "config": {
          "choices": {
            "big_centered_text": {
              "type": "SharedSlice"
            }
          }
        }
      }
    },
    "SEO & Metadata": {
      "meta_title": {
        "config": {
          "label": "Meta Title",
          "placeholder": "A title of the page used for social media and search engines"
        },
        "type": "Text"
      },
      "meta_description": {
        "config": {
          "label": "Meta Description",
          "placeholder": "A brief summary of the page"
        },
        "type": "Text"
      },
      "meta_image": {
        "config": {
          "label": "Meta Image",
          "constraint": {
            "height": 1260,
            "width": 2400
          },
          "thumbnails": []
        },
        "type": "Image"
      }
    }
  }
}
